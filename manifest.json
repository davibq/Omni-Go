{
  "manifest_version": 2,
  "name": "Omni Go!",
  "version": "1.0",
  "author": "David Ballesteros <davibq@gmail.com>",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "omnibox": { "keyword" : "go" },
  "background": {
    "persistent": false,
    "scripts": ["scripts/omnibox.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ]
}