{
  "manifest_version": 2,
  "name": "Omni Go!",
  "version": "1.0",
  "author": "David Ballesteros <davibq@gmail.com>",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "omnibox": { "keyword" : "go" },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ]
}